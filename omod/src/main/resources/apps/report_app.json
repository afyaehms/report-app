[
  {
    "id": "reportapp.reportapp",        // unique id for this app, unique across all apps, typically starts with your module id
    "description": "App for viewing various reports",        // plain text or a message code (not displayed to end-users)
    "extensions": [
      {
        "id": "reportapp.reportAppHomepageLink",
        "extensionPointId": "org.openmrs.referenceapplication.homepageLink",
        "type": "link",
        "label": "Report",        // text or message code (this is shown to the user)
        "url": "reportapp/reportList.page",        // clicking on the app icon goes to this link,
        "order": 10,
        "icon": "icon-tasks",        // see the uicommons style guide icons
        "requiredPrivilege": "Access Report"        // require this privilege to show the link
      }
    ]
  }
]
